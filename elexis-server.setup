<?xml version="1.0" encoding="UTF-8"?>
<setup:Project
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:git="http://www.eclipse.org/oomph/setup/git/1.0"
    xmlns:jdt="http://www.eclipse.org/oomph/setup/jdt/1.0"
    xmlns:pde="http://www.eclipse.org/oomph/setup/pde/1.0"
    xmlns:predicates="http://www.eclipse.org/oomph/predicates/1.0"
    xmlns:projects="http://www.eclipse.org/oomph/setup/projects/1.0"
    xmlns:setup="http://www.eclipse.org/oomph/setup/1.0"
    xmlns:setup.p2="http://www.eclipse.org/oomph/setup/p2/1.0"
    xmlns:setup.workingsets="http://www.eclipse.org/oomph/setup/workingsets/1.0"
    xsi:schemaLocation="http://www.eclipse.org/oomph/setup/git/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Git.ecore http://www.eclipse.org/oomph/setup/jdt/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/JDT.ecore http://www.eclipse.org/oomph/setup/pde/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/PDE.ecore http://www.eclipse.org/oomph/predicates/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Predicates.ecore http://www.eclipse.org/oomph/setup/projects/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Projects.ecore http://www.eclipse.org/oomph/setup/workingsets/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/SetupWorkingSets.ecore"
    name="elexis-server"
    label="Elexis-Server">
  <setupTask
      xsi:type="setup.p2:P2Task">
    <requirement
        name="org.eclipse.cbi.targetplatform-feature.feature.group"/>
    <requirement
        name="org.sonatype.tycho.m2e.feature.feature.group"/>
    <requirement
        name="org.eclipse.m2e.feature.feature.group"/>
    <requirement
        name="org.eclipse.rap.e4.tooling.feature.feature.group"/>
    <requirement
        name="org.eclipse.rap.tools.feature.feature.group"/>
    <requirement
        name="org.eclipse.e4.core.tools.feature.feature.group"/>
    <requirement
        name="net.jeeeyul.pdetools.feature.feature.group"
        filter=""/>
    <requirement
        name="org.eclipse.mylyn.wikitext_feature.feature.group"/>
    <requirement
        name="name.schedenig.eclipse.grepconsole.feature.group"/>
    <repository
        url="http://download.eclipse.org/cbi/tpd/3.0.0-SNAPSHOT/"/>
    <repository
        url="https://repo1.maven.org/maven2/.m2e/connectors/m2eclipse-tycho/0.9.0/N/LATEST/"/>
    <repository
        url="http://download.eclipse.org/rt/rap/incubator/nightly/e4/tooling/site/"/>
    <repository
        url="http://eclipse.jeeeyul.net/update/"/>
    <repository
        url="http://eclipse.schedenig.name"/>
    <description>Install the tools needed in the IDE to work with the source code for ${scope.project.label}</description>
  </setupTask>
  <setupTask
      xsi:type="git:GitCloneTask"
      id="elexis-server.git.clone"
      remoteURI="elexis/elexis-server">
    <annotation
        source="http://www.eclipse.org/oomph/setup/InducedChoices">
      <detail
          key="inherit">
        <value>github.remoteURIs</value>
      </detail>
      <detail
          key="label">
        <value>${scope.project.label} Github repository</value>
      </detail>
      <detail
          key="target">
        <value>remoteURI</value>
      </detail>
    </annotation>
    <description>${scope.project.label}</description>
  </setupTask>
  <setupTask
      xsi:type="setup:CompoundTask"
      name="Common">
    <setupTask
        xsi:type="setup:EclipseIniTask"
        option="-Doomph.redirection.elexis-serversetup"
        value="=https://raw.githubusercontent.com/elexis/elexis-server/3.9/elexis-server.setup->${elexis-server.git.clone.location|uri}/elexis-server.setup"
        vm="true"/>
    <setupTask
        xsi:type="setup:EclipseIniTask"
        option="-Xmx"
        value="1024m"
        vm="true">
      <description>Set the heap space needed to work with the projects of ${scope.project.label}</description>
    </setupTask>
    <setupTask
        xsi:type="setup:ResourceCreationTask"
        excludedTriggers="STARTUP MANUAL"
        content="&lt;?xml version=&quot;1.0&quot; encoding=&quot;UTF-8&quot;?>&#xD;&#xA;&lt;section name=&quot;Workbench&quot;>&#xD;&#xA;&#x9;&lt;section name=&quot;org.eclipse.jdt.internal.ui.packageview.PackageExplorerPart&quot;>&#xD;&#xA;&#x9;&#x9;&lt;item value=&quot;true&quot; key=&quot;group_libraries&quot;/>&#xD;&#xA;&#x9;&#x9;&lt;item value=&quot;false&quot; key=&quot;linkWithEditor&quot;/>&#xD;&#xA;&#x9;&#x9;&lt;item value=&quot;2&quot; key=&quot;layout&quot;/>&#xD;&#xA;&#x9;&#x9;&lt;item value=&quot;2&quot; key=&quot;rootMode&quot;/>&#xD;&#xA;&#x9;&#x9;&lt;item value=&quot;&amp;lt;?xml version=&amp;quot;1.0&amp;quot; encoding=&amp;quot;UTF-8&amp;quot;?&amp;gt;&amp;#x0D;&amp;#x0A;&amp;lt;packageExplorer configured=&amp;quot;true&amp;quot; group_libraries=&amp;quot;1&amp;quot; layout=&amp;quot;2&amp;quot; linkWithEditor=&amp;quot;0&amp;quot; rootMode=&amp;quot;2&amp;quot; sortWorkingSets=&amp;quot;false&amp;quot; workingSetName=&amp;quot;&amp;quot;&amp;gt;&amp;#x0D;&amp;#x0A;&amp;lt;localWorkingSetManager&amp;gt;&amp;#x0D;&amp;#x0A;&amp;lt;workingSet editPageId=&amp;quot;org.eclipse.jdt.internal.ui.OthersWorkingSet&amp;quot; factoryID=&amp;quot;org.eclipse.ui.internal.WorkingSetFactory&amp;quot; id=&amp;quot;1382792884467_1&amp;quot; label=&amp;quot;Other Projects&amp;quot; name=&amp;quot;Other Projects&amp;quot;/&amp;gt;&amp;#x0D;&amp;#x0A;&amp;lt;/localWorkingSetManager&amp;gt;&amp;#x0D;&amp;#x0A;&amp;lt;activeWorkingSet workingSetName=&amp;quot;Other Projects&amp;quot;/&amp;gt;&amp;#x0D;&amp;#x0A;&amp;lt;allWorkingSets workingSetName=&amp;quot;Other Projects&amp;quot;/&amp;gt;&amp;#x0D;&amp;#x0A;&amp;lt;/packageExplorer&amp;gt;&quot; key=&quot;memento&quot;/>&#xD;&#xA;&#x9;&lt;/section>&#xD;&#xA;&lt;/section>&#xD;&#xA;"
        targetURL="${workspace.location|uri}/.metadata/.plugins/org.eclipse.jdt.ui/dialog_settings.xml"
        encoding="UTF-8">
      <description>Initialize JDT's package explorer to show working sets as its root objects</description>
    </setupTask>
  </setupTask>
  <setupTask
      xsi:type="jdt:JRETask"
      version="JavaSE-1.8"
      location="${jre.location-1.8}">
    <description>Define the JRE needed to compile and run the Java projects of ${scope.project.label}</description>
  </setupTask>
  <setupTask
      xsi:type="setup.workingsets:WorkingSetTask">
    <workingSet
        name="bundles"
        id="bundles">
      <predicate
          xsi:type="predicates:LocationPredicate"
          pattern=".*/bundles/.*"/>
    </workingSet>
    <workingSet
        name="features"
        id="features">
      <predicate
          xsi:type="predicates:LocationPredicate"
          pattern=".*/features/.*"/>
    </workingSet>
    <workingSet
        name="tests"
        id="tests">
      <predicate
          xsi:type="predicates:LocationPredicate"
          pattern=".*/tests/.*"/>
    </workingSet>
    <workingSet
        name="releng">
      <predicate
          xsi:type="predicates:LocationPredicate"
          pattern=".*/releng/.*"/>
    </workingSet>
    <workingSet
        name="products">
      <predicate
          xsi:type="predicates:LocationPredicate"
          pattern=".*/products/.*"/>
    </workingSet>
  </setupTask>
  <setupTask
      xsi:type="pde:TargetPlatformTask"
      id="es.target.elexis-server.target"
      name="Elexis-Server"/>
  <setupTask
      xsi:type="projects:ProjectsImportTask">
    <sourceLocator
        rootFolder="${elexis-server.git.clone.location}"
        locateNestedProjects="true"/>
  </setupTask>
  <setupTask
      xsi:type="jdt:JRETask"
      version="JavaSE-9"
      location="${jre.location-9}"/>
  <stream name="3.9"
      label="3.9">
    <setupTask
        xsi:type="setup:CompoundTask"
        name="User Preferences">
      <annotation
          source="http://www.eclipse.org/oomph/setup/UserPreferences">
        <detail
            key="/instance/name.schedenig.eclipse.grepconsole/expressions">
          <value>record</value>
        </detail>
      </annotation>
      <setupTask
          xsi:type="setup:CompoundTask"
          name="name.schedenig.eclipse.grepconsole">
        <setupTask
            xsi:type="setup:PreferenceTask"
            key="/instance/name.schedenig.eclipse.grepconsole/expressions"
            value="&lt;?xml version=&quot;1.0&quot; encoding=&quot;UTF-8&quot; standalone=&quot;no&quot;?>&lt;folders xmlns=&quot;http://eclipse.musgit.com/grepconsole&quot;>&lt;style background=&quot;ff0000&quot; bold=&quot;false&quot; border=&quot;false&quot; id=&quot;8720d322-7fd4-4bac-84cb-71a89591bbbb&quot; italic=&quot;false&quot; name=&quot;Error&quot; strikeout=&quot;false&quot; underline=&quot;false&quot;/>&lt;style bold=&quot;false&quot; border=&quot;false&quot; foreground=&quot;7f7f7f&quot; id=&quot;539dd97b-36dc-4d5f-9fd2-14e94d9b015d&quot; italic=&quot;false&quot; name=&quot;Debug&quot; strikeout=&quot;false&quot; underline=&quot;false&quot;/>&lt;style bold=&quot;false&quot; border=&quot;false&quot; foreground=&quot;2f98f2&quot; id=&quot;6cfd15b2-6a74-457b-bf8c-0325d26a2643&quot; italic=&quot;false&quot; name=&quot;Info&quot; strikeout=&quot;false&quot; underline=&quot;false&quot;/>&lt;style bold=&quot;false&quot; border=&quot;false&quot; id=&quot;1b0cf990-ad2d-4bc0-81e6-f59d7131f5a6&quot; italic=&quot;false&quot; strikeout=&quot;false&quot; underline=&quot;false&quot;/>&lt;style background=&quot;a52a2a&quot; bold=&quot;true&quot; border=&quot;false&quot; foreground=&quot;ffff00&quot; id=&quot;79ade458-a1ae-498b-9d83-a6a9f469185f&quot; italic=&quot;false&quot; name=&quot;Fatal&quot; strikeout=&quot;false&quot; underline=&quot;false&quot;/>&lt;style background=&quot;ffd27f&quot; bold=&quot;false&quot; border=&quot;false&quot; id=&quot;2d9ece6e-0325-4d39-8579-4456851f259c&quot; italic=&quot;false&quot; name=&quot;Warn&quot; strikeout=&quot;false&quot; underline=&quot;false&quot;/>&lt;folder enabled=&quot;true&quot; filter=&quot;true&quot; id=&quot;4d3fe9d9-d0e7-4536-bcf9-751170c3c98e&quot; notifications=&quot;true&quot; statistics=&quot;true&quot;>&lt;folder enabled=&quot;true&quot; id=&quot;75da4f67-40e8-45e0-8c75-8292c6f9e5db&quot; name=&quot;Log Output&quot;>&lt;item enabled=&quot;true&quot; expression=&quot;.*(\QDEBUG\E).*&quot; id=&quot;733554cb-6759-46e4-96b4-05b0ae9686e8&quot; name=&quot;Debug&quot;>&lt;group style=&quot;539dd97b-36dc-4d5f-9fd2-14e94d9b015d&quot;/>&lt;group/>&lt;/item>&lt;item enabled=&quot;true&quot; expression=&quot;.*(\QWARN\E).*&quot; id=&quot;805195bb-ae5d-4b68-b2c8-11e4688de116&quot; name=&quot;Warn&quot;>&lt;group style=&quot;2d9ece6e-0325-4d39-8579-4456851f259c&quot;/>&lt;group/>&lt;/item>&lt;item enabled=&quot;true&quot; expression=&quot;.*(\QERROR\E).*&quot; id=&quot;0f03e7f0-7082-462f-ae84-1883904b5301&quot; name=&quot;Error&quot;>&lt;group style=&quot;8720d322-7fd4-4bac-84cb-71a89591bbbb&quot;/>&lt;group/>&lt;/item>&lt;item expression=&quot;.*(\Q[FATAL]\E).*&quot; id=&quot;6da0cf2f-86fe-4054-9f74-522cb8137a72&quot; name=&quot;Fatal&quot;>&lt;group style=&quot;79ade458-a1ae-498b-9d83-a6a9f469185f&quot;/>&lt;group/>&lt;/item>&lt;item enabled=&quot;true&quot; expression=&quot;.*(\QINFO\E).*&quot; id=&quot;be290cbd-e7fe-401a-9972-88dab80bc3a8&quot; name=&quot;Info&quot;>&lt;group style=&quot;6cfd15b2-6a74-457b-bf8c-0325d26a2643&quot;/>&lt;group/>&lt;/item>&lt;/folder>&lt;/folder>&lt;/folders>"/>
      </setupTask>
    </setupTask>
  </stream>
  <logicalProjectContainer
      xsi:type="setup:ProjectCatalog"
      href="index:/org.eclipse.setup#//@projectCatalogs[name='com.github']"/>
  <description>
    ElexisBox a headless Elexis version suitable 
    for server operation.
  </description>
</setup:Project>
